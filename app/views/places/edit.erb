<h2>Edit A New Place</h2>
<br>
<form action="/places/<%=@id%>/update" method="get">
  <div class="row">
    <div class="col-xs-2">
      <strong>
        Title:<br>
        <br>
        Photo URL:<br>
        <br>
        Price ($):<br>
        <br>
      </strong>
    </div>
    <div class="col-xs-10">
      <input type="text" name="title" size="50" value="<%=@title%>"><br>
      <br>
      <input type="text" name="photo_url" size="100" value="<%=@photo_url%>"><br>
      <br>
      <input type="text" name="price" value="<%=@admission_price%>">
      <br>
    </div>
  </div>
  <hr>
  <strong>Description</strong><br><br>
  <textarea name="description" rows="4" cols="50"><%=@description%></textarea>
  <br><br>
  <button class="btn btn-primary" type="submit">Update</button>
</form>