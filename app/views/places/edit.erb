<h2>Edit A New Place</h2>
<br>
<form action="/places/<%=@id%>/update" method="get">
  <div>
    <div>
      Title:<input type="text" name="title" size="50" value="<%=@title%>"><br><br>
      Photo:<input type="text" name="photo_url" size="50" value="<%=@photo_url%>"><br><br>
      Price:<input type="text" name="price" size="50" value="<%=@admission_price%>"><br><br>
    </div>
  Description<br><br>
  <textarea name="description" rows="4" cols="50"><%=@description%></textarea>
  <br><br>
  <button class="btn btn-primary" type="submit">Update</button>
  </div>
</form>